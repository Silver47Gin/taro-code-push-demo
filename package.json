{
  "name": "taro-code-push-demo",
  "version": "0.0.9",
  "private": true,
  "templateInfo": {
    "name": "react-native",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "prepare": "husky install",
    "podInstall": "pod-install",
    "upgradePeerdeps": "install-peerdeps @tarojs/taro-rn@3.3.9 -o -Y && install-peerdeps @tarojs/components-rn@3.3.9 -o -Y && install-peerdeps @tarojs/router-rn@3.3.9 -o -Y && pod-install",
    "server": "nodemon server/index.ts",
    "iconfont": "iconfont-taro",
    "start": "taro build --type rn --watch --reset-cache --qr",
    "android": "react-native run-android --no-packager",
    "build:android": "cd android && ./gradlew assemble",
    "install:android": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "bundle:android": "taro build --type rn --platform android --reset-cache --config rn.output.android=.temp/android/index.android.bundle --config rn.output.androidAssetsDest=.temp/android --config rn.output.androidSourcemapOutput=.temp/android/index.android.bundle.map",
    "codepush:android": "appcenter codepush release -c .temp/android -a \"$CODE_PUSH_USERNAME/$ANDROID_CODE_PUSH_APP\" -t \"*\" -d Production",
    "ios": "react-native run-ios --no-packager",
    "build:ios": "xcodebuild -workspace ios/taroDemo.xcworkspace -scheme taroDemo clean archive -configuration release -archivePath .temp/build/taroDemo.xcarchive",
    "export:ios": "xcodebuild -exportArchive -archivePath .temp/build/taroDemo.xcarchive -exportPath .temp/build/taroDemo.ipa -exportOptionsPlist a.plist",
    "bundle:ios": "taro build --type rn --platform ios --reset-cache --config rn.output.ios=.temp/ios/main.jsbundle --config rn.output.iosAssetsDest=.temp/ios --config rn.output.iosSourcemapOutput .temp/ios/main.map",
    "codepush:ios": "appcenter codepush release -c .temp/ios -a \"$CODE_PUSH_USERNAME/$IOS_CODE_PUSH_APP\" -t \"*\" -d Production"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@tarojs/components": "3.3.9",
    "@tarojs/react": "3.3.9",
    "@tarojs/runtime": "3.3.9",
    "@tarojs/taro": "3.3.9",
    "@tarojs/taro-rn": "3.3.9",
    "@types/express": "^4.17.13",
    "dayjs": "^1.10.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "husky": "^7.0.2",
    "mobx": "^6.3.3",
    "mobx-persist": "^0.4.1",
    "mobx-react-lite": "^3.2.1",
    "mobx-utils": "^6.0.4",
    "multer": "^1.4.3",
    "nanoid": "^3.1.29",
    "prettier": "^2.3.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-native": "^0.64.0",
    "react-native-amap3d": "^3.0.1",
    "react-native-code-push": "^7.0.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg": "^12.1.1",
    "rxjs": "^6.6.3",
    "styled-components": "^5.3.3",
    "ts-enum-util": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@tarojs/cli": "^3.3.9",
    "@tarojs/mini-runner": "3.3.9",
    "@tarojs/rn-runner": "3.3.9",
    "@tarojs/webpack-runner": "3.3.9",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.66.4",
    "@types/styled-components": "^5.1.15",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.3.9",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.3.9",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "install-peerdeps": "^3.0.3",
    "nodemon": "^2.0.15",
    "pod-install": "^0.1.23",
    "stylelint": "9.3.0",
    "taro-iconfont-cli": "^3.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.0"
  }
}
